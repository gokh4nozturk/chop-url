{
  "name": "@chop-url/backend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "setup": "ts-node src/setup-db.ts",
    "deploy": "wrangler deploy",
    "migrate": "wrangler d1 execute chop-url-db --file=schema.sql --remote",
    "db:reset": "wrangler d1 execute chop-url-db --file=schema.sql"
  },
  "keywords": [
    "url-shortener",
    "cloudflare-workers",
    "typescript"
  ],
  "author": "Gokhan Ozturk",
  "license": "MIT",
  "dependencies": {
    "@chop-url/lib": "workspace:*",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "hono": "^4.6.20",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250129.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.17",
    "@types/pg": "^8.11.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.107.3"
  }
}

{
  "version": 2,
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "git": {
    "deploymentEnabled": {
      "master": true
    }
  },
  "github": {
    "silent": true
  },
  "cleanUrls": true,
  "trailingSlash": false
}
